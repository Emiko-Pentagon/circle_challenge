{
  "name": "etherfantasy-api",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon -e js,json,ts --exec ts-node-dev app.ts",
    "copy": "cp .env dist/ && cp -r public dist/ && cp ecosystem.config.js dist/",
    "build": "rm -rf dist && tsc && npm run copy",
    "seed": "ts-node api/seeds",
    "test:clearCache": "node_modules/.bin/jest --clearCache && rm -rf ./dist",
    "test:all": "node_modules/.bin/jest --watchAll --runInBand --detectOpenHandles",
    "test:validation": "node_modules/.bin/jest --testPathPattern=validate.spec.ts --watchAll",
    "test:category": "node_modules/.bin/jest --testPathPattern=category.spec.ts --watchAll",
    "test:product": "node_modules/.bin/jest --testPathPattern=product.spec.ts --watchAll",
    "test:employee": "node_modules/.bin/jest --testPathPattern=employee.spec.ts --watchAll",
    "test:profile": "node_modules/.bin/jest --testPathPattern=profile.spec.ts --watchAll",
    "test:ecommerce": "node_modules/.bin/jest --testPathPattern=ecommerce.spec.ts --watchAll",
    "test:paypal": "node_modules/.bin/jest --testPathPattern=paypal.spec.ts --watchAll",
    "test:csv-json": "node_modules/.bin/jest --testPathPattern=\"(csv-to-json|json-to-csv).spec.ts\" --watchAll"
  },
  "author": "",
  "license": "ISC",
